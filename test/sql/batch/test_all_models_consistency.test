# name: test/sql/batch/test_all_models_consistency.test
# description: Comprehensive batch consistency test for all working models
# group: [batch]
# This test verifies that models known to work correctly maintain batch/individual consistency

require anofox_forecast

statement ok
LOAD anofox_forecast;

# Create test dataset with 2 series: one constant, one with trend
statement ok
CREATE TABLE comprehensive_test AS
SELECT 'ConstantSeries' AS id, i::DATE AS date, 100.0::DOUBLE AS value
FROM generate_series(DATE '2023-01-01', DATE '2024-01-01', INTERVAL 1 DAY) t(i)
UNION ALL
SELECT 'TrendSeries' AS id, i::DATE AS date, (100.0 + (i - DATE '2023-01-01')::INT * 0.5)::DOUBLE AS value
FROM generate_series(DATE '2023-01-01', DATE '2024-01-01', INTERVAL 1 DAY) t(i);

# Test Model 1: Naive (KNOWN WORKING)
statement ok
CREATE OR REPLACE TABLE test_batch AS
SELECT id, point_forecast FROM TS_FORECAST_BY(comprehensive_test, id, date, value, 'Naive', 14, NULL);

statement ok
CREATE OR REPLACE TABLE test_individual AS
SELECT 'ConstantSeries' AS id, point_forecast FROM ts_forecast((SELECT date, value FROM comprehensive_test WHERE id = 'ConstantSeries'), date, value, 'Naive', 14, NULL)
UNION ALL
SELECT 'TrendSeries' AS id, point_forecast FROM ts_forecast((SELECT date, value FROM comprehensive_test WHERE id = 'TrendSeries'), date, value, 'Naive', 14, NULL);

query I
SELECT COUNT(*) = 0 FROM (
    SELECT b.id, b.point_forecast AS batch_fcst, i.point_forecast AS ind_fcst, row_number() OVER (PARTITION BY b.id) AS rn
    FROM test_batch b JOIN test_individual i ON b.id = i.id
) WHERE ABS(batch_fcst - ind_fcst) > 0.01;
----
true

