    CASE WHEN i BETWEEN 5 AND 7 THEN NULL ELSE 100.0 END AS value
FROM generate_series(1, 30) t(i);

query I
SELECT SUM(CASE WHEN value IS NULL THEN 1 ELSE 0 END) < 3
FROM anofox_fcst_ts_fill_nulls_forward('nulls_ts', 'series_id', 'date_col', 'value');
----
true

