# name: test/sql/core/test_comprehensive_date_types.test
# description: Comprehensive test for INTEGER, DATE, and TIMESTAMP support across all functions
# group: [core]

require anofox_forecast

statement ok
LOAD anofox_forecast;

#############################################
# FORECASTING FUNCTIONS - anofox_fcst_ts_forecast
#############################################

# Test anofox_fcst_ts_forecast with INTEGER
statement ok
CREATE TABLE test_int_single AS
SELECT i AS date_col, (100.0 + i * 5)::DOUBLE AS value
FROM generate_series(1, 30) t(i);

query I
SELECT COUNT(*) = 10
FROM anofox_fcst_ts_forecast(test_int_single, date_col, value, 'Naive', 10, NULL);
----
true

# Test anofox_fcst_ts_forecast with DATE
statement ok
CREATE TABLE test_date_single AS
SELECT ('2024-01-01'::DATE + INTERVAL (i) DAY)::DATE AS date_col, (100.0 + i * 5)::DOUBLE AS value
FROM generate_series(1, 30) t(i);

query I
SELECT COUNT(*) = 10
FROM anofox_fcst_ts_forecast(test_date_single, date_col, value, 'Naive', 10, NULL);
----
true

# Test anofox_fcst_ts_forecast with TIMESTAMP
statement ok
CREATE TABLE test_ts_single AS
