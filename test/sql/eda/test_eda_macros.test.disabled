# Test: EDA workflow - chain multiple analyses
statement ok
CREATE TABLE complete_eda AS
SELECT 
    s.series_id,
    s.length AS n_points,
    s.mean_value AS mean,
    s.std_value AS std_dev,
    q.n_null AS null_count,
    q.n_zeros AS zero_count,
    q.is_constant,
    g.n_gaps AS gap_count
FROM stats_result s
JOIN quality_result q ON s.series_id = q.series_id
JOIN gaps_result g ON s.series_id = g.series_id;

# Verify complete EDA results
query I
SELECT COUNT(*) = 2
FROM complete_eda;
----
true

# Verify series_1 has data quality issues
query I
SELECT 
    null_count > 0 
    AND zero_count > 0 
    AND gap_count > 0
    AND is_constant = false
FROM complete_eda
WHERE series_id = 'series_1';
----
true

# Verify series_2 is constant with no issues
query I
SELECT 
    null_count = 0 
    AND zero_count = 0 
    AND gap_count = 0
    AND is_constant = true
FROM complete_eda
WHERE series_id = 'series_2';
----
true

