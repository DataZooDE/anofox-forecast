# Signature: ts_analyze_zeros(table_name, group_cols, date_col, value_col)
statement ok
CREATE TABLE zeros_result AS
SELECT * FROM ts_analyze_zeros(eda_test_data, series_id, date, value);

# Verify zeros are detected in series_1
query I
